<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ht.dao.PicDao">
    <insert id="addpic">
        insert into pictures values(null,#{pname},#{psrc},#{ptime})
    </insert>

    <select id="slepic" resultType="pictures">
        select * from pictures
    </select>
</mapper>